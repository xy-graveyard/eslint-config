{
  "$schema": "http://json.schemastore.org/package.json",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/eslint-config/issues"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-json-es": "^1.5.4",
    "eslint-plugin-md": "^1.0.19",
    "eslint-plugin-no-secrets": "^0.8.9",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "remark-preset-lint-markdown-style-guide": "^5.1.2"
  },
  "description": "ESLint Config used throughout XY Labs TypeScript/JavaScript libraries and react projects",
  "devDependencies": {
    "eslint": "^8",
    "prettier": "^2",
    "typescript": "^4"
  },
  "homepage": "https://github.com/xylabs/eslint-config.git",
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "eslint"
  ],
  "license": "LGPL-3.0",
  "main": "./index.json",
  "name": "@xylabs/eslint-config",
  "peerDependencies": {
    "eslint": "^8",
    "prettier": "^2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/eslint-config.git"
  },
  "scripts": {
    "deploy": "yarn version --patch && yarn publish ./",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install"
  },
  "sideEffects": false,
  "version": "2.1.9"
}
