{
  "$schema": "http://json.schemastore.org/package.json",
  "author": {
    "email": "support@xylabs.com",
    "name": "XY Labs Development Team",
    "url": "https://xylabs.com"
  },
  "bugs": {
    "email": "support@xylabs.com",
    "url": "https://github.com/xylabs/eslint-config/issues"
  },
  "dependencies": {
    "@babel/core": "^7",
    "@babel/eslint-parser": "^7",
    "@rushstack/eslint-patch": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-import-resolver-typescript": "^2",
    "eslint-plugin-import": "^2",
    "eslint-plugin-json-es": "^1",
    "eslint-plugin-md": "^1",
    "eslint-plugin-no-secrets": "^0",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-simple-import-sort": "^7",
    "eslint-plugin-sort-keys-fix": "^1",
    "markdown-eslint-parser": "^1.2.1",
    "prettier": "^2",
    "remark-preset-lint-markdown-style-guide": "^5"
  },
  "description": "ESLint Config used throughout XY Labs TypeScript/JavaScript libraries and react projects",
  "devDependencies": {
    "@types/node": "^17",
    "@xylabs/tsconfig": "^1.0.9",
    "rimraf": "^3.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4"
  },
  "homepage": "https://github.com/xylabs/eslint-config.git",
  "keywords": [
    "xylabs",
    "utility",
    "typescript",
    "eslint"
  ],
  "license": "LGPL-3.0",
  "main": "./dist/index.js",
  "name": "@xylabs/eslint-config",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/eslint-config.git"
  },
  "scripts": {
    "build": "tsc",
    "deploy": "yarn version --patch && yarn publish ./",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "reinstall": "rimraf ./node_modules && rimraf ./yarn.lock && rimraf ./yarn-error.log && yarn install"
  },
  "sideEffects": false,
  "version": "2.3.2"
}
