{
  "extends": [
    "plugin:md/recommended"
  ],
  "overrides": [
    {
      "files": ["*.md"],
      "parser": "markdown-eslint-parser",
      "rules": {
        "prettier/prettier": [
          "error",
          { "parser": "markdown" }
        ]
      }
    }
  ],
  "rules": {
    "import/no-internal-modules": [
      "warn", {
        "allow": [

        ]
      }
    ],
    "md/remark": [
      "error",
      {
        "plugins": ["preset-lint-markdown-style-guide", "frontmatter", ["lint-maximum-line-length", false], ["lint-list-item-indent", false]]
      }
    ]
  }
}